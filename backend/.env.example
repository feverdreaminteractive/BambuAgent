# BambuAgent Configuration

# Claude AI API Key (get from https://console.anthropic.com)
ANTHROPIC_API_KEY=your_claude_api_key_here

# Bambu A1 mini Configuration
BAMBU_PRINTER_IP=192.168.1.100          # Your printer's IP address
BAMBU_ACCESS_CODE=12345678               # Access code from printer LCD menu
BAMBU_DEVICE_SERIAL=your_printer_serial  # Optional: printer serial number

# Optional: Development Settings
DEBUG=true
LOG_LEVEL=INFO